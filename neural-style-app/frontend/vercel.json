{
  "version": 2,
  "builds": [
    {
      "src": "neural-style-app/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "neural-style-app/frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/neural-style-app/frontend/dist/$1"
    }
  ]
}
